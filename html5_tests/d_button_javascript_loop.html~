<head>
<script>
function  BSLine(lineName, value)
{
	this.lineName = lineName;
	this.value = value;
}

function myFunction()
{


	var balanceSheet = new Array();

	balanceSheet.push(new BSLine("Inmovilizado", 10000));

	for (o in balanceSheet)
	{
		var lineName = o.lineName;
		console.log(lineName);
		console.log(o.lineName + " " + o.value);
	}
}
</script>
</head>

<body>

<h1>My Web Page</h1>

<p id="demo">A Paragraph</p>

<button type="button" onclick="myFunction()">Try it</button>

